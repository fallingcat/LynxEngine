--
-- 	class CDynamicObj
-------------------------------------------
CDynamicObj = class(CGameObj)
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:ctor(x)	
	self.x=x
end
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:Forward(f) 
	_CDynamicObj:Forward(f) 
end		
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:Yaw(d, o) 
	_CDynamicObj:Yaw(d, o) 
end		
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:Pitch(d, o) 
	_CDynamicObj:Pitch(d, o) 
end		
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:UpdateTransform() 
	_CDynamicObj:UpdateTransform() 
end			
-----------------------------------------	
--
-----------------------------------------	
function CDynamicObj:Add(x, y)
	return x+y
end			


